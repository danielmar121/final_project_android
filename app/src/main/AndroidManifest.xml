<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daniel.final_project">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".activities.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Final_project">

        <activity android:name=".activities.LandingPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.SignUpActivity" />

        <activity android:name=".activities.buyer.BuyerMainActivity" />
        <activity android:name=".activities.buyer.ui.cart.ProductsOrderBuyer" />
        <activity android:name=".activities.buyer.ui.cart.BuyerPaymentActivity" />
        <activity android:name=".activities.buyer.ui.shops.ShopBuyer" />

        <activity android:name=".activities.supplier.SupplierMainActivity" />
        <activity android:name=".activities.supplier.SupplierSignUpActivity" />
        <activity android:name=".activities.MainActivity" />
    </application>
</manifest>